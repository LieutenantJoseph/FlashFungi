{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/admin",
      "dest": "/docs/admin.html"
    },
    {
      "src": ".*\\.js$",
      "dest": "/docs$0",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8"
      }
    },
    {
      "src": ".*\\.css$",
      "dest": "/docs$0",
      "headers": {
        "Content-Type": "text/css; charset=utf-8"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/docs/manifest.json"
    },
    {
      "src": "/",
      "dest": "/docs/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/index.html"
    }
  ]
}