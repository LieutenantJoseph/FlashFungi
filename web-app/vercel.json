{
  "functions": {
    "api/admin.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/admin",
      "dest": "/docs/admin.html"
    },
    {
      "src": "/",
      "dest": "/docs/index.html"
    },
    {
      "src": "/(.*\\.(js|css|html|png|jpg|svg|ico))",
      "dest": "/docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/index.html"
    }
  ],
  "env": {
    "SUPABASE_SERVICE_KEY": "@supabase-service-key"
  }
}